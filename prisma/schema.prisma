
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model trip {
  id String @id @default(uuid())
  destination String
  starts_at DateTime
  ends_at DateTime
  is_confirmed Boolean @default(false)
  create_at DateTime @default(now())
  
  @@map("trips")
}
